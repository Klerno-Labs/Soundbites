{
  "routes": [
    {
      "type": "rewrite",
      "source": "/admin",
      "destination": "/Admin%20App/Soundbites%20Admin/admin.html"
    },
    {
      "type": "rewrite",
      "source": "/admin/",
      "destination": "/Admin%20App/Soundbites%20Admin/admin.html"
    }
  ],
  "headers": [
    {
      "source": "**/*",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
